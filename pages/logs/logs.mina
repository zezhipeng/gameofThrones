<template lang="pug">
view.container.log-list
  block(wx:for='{{logs}}', wx:for-item='log')
    text.log-item {{index + 1}}. {{log}}

</template>

<script>
const { regeneratorRuntime, util } = global

Page({
  data: {
    logs: []
  },
  onLoad: function () {
    this.setData({
      logs: (wx.getStorageSync('logs') || []).map(function (log) {
        return util.formatTime(new Date(log))
      })
    })
  }
})
</script>
<style lang="sass">
.log-list
  color: #666
</style>